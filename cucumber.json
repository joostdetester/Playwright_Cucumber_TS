{
    "default": {
        "tags": "@e2e",
        "formatOptions": {
            "snippetInterface": "async-await"
        },
        "paths": [
            "src/test/features/",
            "src/test/features/ATS",
            "src/test/features/CRM"
        ],
        "publishQuiet": true,
        "dryRun": false,
        "require": [
            "src/test/steps/*.ts",
            "src/test/steps/ATS/*.ts",
            "src/test/steps/CRM/*.ts",
            "src/hooks/hooks.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            "progress-bar",
            "html:test-results/cucumber-report.html",
            "json:test-results/cucumber-report.json",
            "rerun:@rerun.txt"
        ],
        "parallel": 2
    },
    "rerun": {
        "formatOptions": {
            "snippetInterface": "async-await"
        },
        "publishQuiet": true,
        "dryRun": true,
        "require": [
            "src/test/steps/*.ts",
            "src/test/steps/ATS/*.ts",
            "src/test/steps/CRM/*.ts",
            "src/hooks/hooks.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            "progress-bar",
            "html:test-results/cucumber-report.html",
            "json:test-results/cucumber-report.json",
            "rerun:@rerun.txt"
        ],
        "parallel": 2
    }
}